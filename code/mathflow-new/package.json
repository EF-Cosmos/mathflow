{
  "name": "mathflow",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "pnpm install --prefer-offline && vite",
    "build": "pnpm install --prefer-offline && node --input-type=module -e \"import fs from 'node:fs'; fs.rmSync('node_modules/.vite-temp', { recursive: true, force: true });\" && tsc -b && vite build",
    "build:prod": "pnpm install --prefer-offline && node --input-type=module -e \"import fs from 'node:fs'; fs.rmSync('node_modules/.vite-temp', { recursive: true, force: true });\" && tsc -b && BUILD_MODE=prod vite build",
    "lint": "pnpm install --prefer-offline && eslint .",
    "preview": "pnpm install --prefer-offline && vite preview"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.45.0",
    "katex": "^0.16.9",
    "@radix-ui/react-slot": "^1.1.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.364.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/node": "^22.10.7",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-router-dom": "^5",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "10.4.20",
    "eslint": "^9.15.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.12.0",
    "postcss": "8.4.49",
    "tailwindcss": "v3.4.16",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.15.0",
    "vite": "^6.0.1"
  }
}
